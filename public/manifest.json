{
    "manifest_version": 3,
    "name": "New Ticket",
    "description": "Create a new case",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "images/hello_extensions.png"
    },
    "icons": {
        "16": "images/logo192.png",
        "48": "images/logo192.png",
        "128": "images/logo192.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ]
}
